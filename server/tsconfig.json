{
    "compilerOptions": {
        "target": "ES2022",
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "module": "ES2022",
        "allowImportingTsExtensions": true,
        "noEmit": true
    },
    "include": ["*.ts", "./**/*.ts"],
    "exclude": ["node_modules"]
}
